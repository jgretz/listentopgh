{
  "name": "truefit-react-template",
  "private": true,
  "scripts": {
    "start": "yarn run generateFiles && yarn run serve",
    "serve": "babel-node tools/server.js",
    "lint": "eslint webpack.config.* app tools --color --fix",
    "pretty": "prettier --write \"**/*.{js,scss}\"",
    "generateIndexFiles": "run-p generateActionIndexFiles generateComponentIndexFiles generateReducerIndexFiles generateServiceIndexFiles generateSelectorIndexFiles",
    "generateActionIndexFiles": "babel-node tools/generateActionIndexFiles.js",
    "generateComponentIndexFiles": "babel-node tools/generateComponentIndexFiles.js",
    "generateServiceIndexFiles": "babel-node tools/generateServiceIndexFiles.js",
    "generateSelectorIndexFiles": "babel-node tools/generateSelectorIndexFiles.js",
    "generateReducerIndexFiles": "babel-node tools/generateReducerIndexFiles.js",
    "generateRootReducer": "babel-node tools/generateRootReducer.js",
    "generateFiles": "run-p generateIndexFiles generateRootReducer",
    "webpack": "webpack-cli --config webpack.config.prod.babel.js --bail",
    "clean": "rimraf ./lib",
    "build": "yarn run generateFiles && yarn run webpack",
    "precommit": "lint-staged && yarn tests",
    "tests": "jest"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --color --fix",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-extended",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@hocs/with-lifecycle": "^0.5.0",
    "babel-polyfill": "^6.26.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-leaflet": "^2.0.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "next",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-async-await": "^1.0.1",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "semantic-ui-react": "^0.82.3",
    "truefit-react-utils": "^0.0.14"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.24.7",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "jest-extended": "^0.8.1",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.2",
    "node-dir": "^0.1.17",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "react-hot-loader": "^4.3.5",
    "redux-devtools-extension": "^2.13.5",
    "redux-immutable": "^4.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.23.0",
    "webpack-stylish": "^0.1.8"
  }
}
