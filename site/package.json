    "class-autobind": "^0.1.4",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "font-awesome": "^4.6.3",
    "history": "^4.6.3",
    "immutable": "^3.8.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.15.1",
    "numeral": "^2.0.4",
    "pluralize": "^7.0.0",
    "react": "^16.2.0",
    "react-addons-shallow-compare": "^15.6.0",
    "react-dom": "^16.2.0",
    "react-google-maps": "^9.4.5",
    "react-loading-overlay": "^0.2.3",
    "react-redux": "^5.0.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "react-spin": "^0.6.2",
    "reactcss": "^1.1.1",
    "redux": "^3.7.2",
    "redux-async-await": "^1.0.1",
    "redux-promise-middleware": "^5.0.0",
