    "eslint-watch": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "node-dir": "^0.1.16",
    "node-sass": "^4.3.0",
    "npm-run-all": "^4.0.1",
    "postcss-loader": "^2.0.10",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.3",
